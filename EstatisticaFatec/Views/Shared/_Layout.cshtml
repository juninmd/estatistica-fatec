<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Estatística Aplicada - Fatec</title>
    @Styles.Render("~/Content/css")
    @Scripts.Render("~/bundles/modernizr")
</head>

<body>
    <div class="navbar navbar-inverse navbar-fixed-top">
        <div class="container">
            <div class="navbar-header">
                @Html.ActionLink("Sistema de Estatísticas", "Index", "Home", null, new { @class = "navbar-brand" })
            </div>

        </div>
    </div>
    <div class="container body-content" style="margin-top: 40px">
        @RenderBody()
        @Html.Partial("_aguarde")
        <hr />
    </div>

    @Scripts.Render("~/bundles/jquery")
    @Scripts.Render("~/bundles/bootstrap")
    <script src="~/Scripts/highcharts.js"></script>
    <script src="~/Scripts/fatec.js"></script>

    <script>
        $(document).ready(function () {
            $('[data-toggle="tooltip"]').tooltip();
            $.material.init();
        });
    </script>
    @RenderSection("scripts", required: false)
</body>



</html>
@* http://fezvrasta.github.io/bootstrap-material-design/#navbar *@
